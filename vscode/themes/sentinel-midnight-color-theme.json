  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#898294",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#f9f8f9"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8a76ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#5ece73"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#fdb81b"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#ff45a8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#bf8600"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#998bff"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "support.class"],
      "settings": {
        "foreground": "#bf8600",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#e7e5ea"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d5d2da"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#d5d2da"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff45a8"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#bf8600"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#8a76ff"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#e7e5ea"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#7466F0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#E1567C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7466F0",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#E1567C"
      }
    },
    {
      "name": "Invalid/Error",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#fe4144",
        "fontStyle": "bold"
      }
    }
  ]
}
