    "editorWidget.background": "#0A0A0A",
    "editorWidget.border": "#181225",
    "editorWidget.foreground": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A8A8B0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.modifier"],
      "settings": {
        "foreground": "#B8A5FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#A5E68A"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Booleans and Language Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#3EA6FF",
        "fontStyle": ""
      }
    },
    {
      "name": "Type and Class Names",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#FF70BC"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#9E86FF"
      }
    },

    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.definition.property",
        "variable.object.property",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#FF9FC7"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF70BC"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#B8B8C8"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FDB81B"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css", 
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "meta.selector.css"
      ],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "CSS Punctuation",
      "scope": [
        "punctuation.definition.entity.css",
        "punctuation.definition.entity.begin.bracket",
        "punctuation.definition.entity.end.bracket"
      ],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "At-rules",
      "scope": [
        "keyword.control.at-rule",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "variable.css", "variable.other.css"],
      "settings": {
        "foreground": "#e7e5ea"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FF70BC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#E1567C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#FF70BC",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#E1567C"
      }
    },
    {
      "name": "Invalid/Error",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#fe4144",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Declaration Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.module",
        "keyword.other.import",
        "keyword.other.package",
        "keyword.other.namespace",
        "storage.type.interface",
        "storage.type.type",
        "storage.type.class",
        "storage.type.function",
        "storage.type.const",
        "storage.type.let",
        "storage.type.var",
        "storage.type.enum",
        "storage.type.namespace",
        "storage.type.module",
        "storage.type.struct",
        "storage.type.trait",
        "storage.type.impl",
        "storage.type.def",
        "storage.modifier.import",
        "storage.modifier.package",
        "keyword.declaration",
        "keyword.function",
        "keyword.class",
        "keyword.interface",
        "keyword.struct",
        "keyword.control.def",
        "keyword.control.class",
        "keyword.other.fn",
        "keyword.other.def",
        "keyword.other.special-method",
        "meta.function.python keyword.python",
        "meta.class.python keyword.python",
        "source.go keyword.function",
        "source.go keyword.type",
        "source.go keyword.var",
        "source.go keyword.const",
        "source.rust storage.type",
        "keyword.other.important"
      ],
      "settings": {
        "foreground": "#A794FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage fallback",
      "scope": ["storage"],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#A794FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#f4e4c9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#f4e4c9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#83da90",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["markup.underline.link", "markup.underline.link.image"],
      "settings": {
        "foreground": "#3EA6FF"
      }
    },
    {
      "name": "Markup - Raw/Code",
      "scope": ["markup.inline.raw", "markup.raw", "markup.raw.inline"],
      "settings": {
        "foreground": "#83da90"
      }
    },
    {
      "name": "Markup - Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#fe4144"
      }
    },
    {
      "name": "Markup - Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#83da90"
      }
    },
    {
      "name": "Markup - Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#f4e4c9"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#f4e4c9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Meta Diff",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#FFFFFF60"
      }
    },

    {
      "name": "Meta Embedded",
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Punctuation Section",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.section.block",
        "punctuation.section.brackets",
        "punctuation.section.array",
        "punctuation.section.function"
      ],
      "settings": {
        "foreground": "#FFFFFF80"
      }
    },
    {
      "name": "Punctuation Terminator",
      "scope": ["punctuation.terminator", "punctuation.separator.statement"],
      "settings": {
        "foreground": "#FFFFFF80"
      }
    },
    {
      "name": "Entity Name Tag",
      "scope": ["entity.name.section", "entity.name.tag", "entity.name.namespace"],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "CSS Pseudo-classes and Pseudo-elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "Library/Support Functions",
      "scope": ["support.function.builtin", "support.function.library"],
      "settings": {
        "foreground": "#cdcbff"
      }
    },
    {
      "name": "Invalid - Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#FFFFFF60",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Regular Expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#67E8F9"
      }
    },
    {
      "name": "Template Expression",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#A794FF"
      }
    },
    {
      "name": "Language Variables",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.self",
        "variable.language.super",
        "keyword.other.this"
      ],
      "settings": {
        "foreground": "#bab6ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameter",
        "entity.name.variable.parameter"
      ],
      "settings": {
        "foreground": "#e1bd7d",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Diff Header",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff",
        "meta.diff.index",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#67E8F9"
      }
    }
  ]
}
