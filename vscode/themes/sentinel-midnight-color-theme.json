    "editorSuggestWidget.highlightForeground": "#FF70BC",
    "editorSuggestWidget.focusHighlightForeground": "#FF70BC",
    
    "editorHoverWidget.background": "#0A0A0A",
    "editorHoverWidget.border": "#181225",
