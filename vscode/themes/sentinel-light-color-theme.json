  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#898294",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#181225"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.modifier"],
      "settings": {
        "foreground": "#4E2A9A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#36166B"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#EE8019"
      }
    },
    {
      "name": "Booleans and Language Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#6E47AE"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#6E47AE",
        "fontStyle": ""
      }
    },
    {
      "name": "Type and Class Names",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#A737B4"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#4E2A9A"
      }
    },

    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.definition.property",
        "variable.object.property",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#36166B"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#36166B"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#EE8019"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css", 
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "meta.selector.css"
      ],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "CSS Punctuation",
      "scope": [
        "punctuation.definition.entity.css",
        "punctuation.definition.entity.begin.bracket",
        "punctuation.definition.entity.end.bracket"
      ],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "At-rules",
      "scope": [
        "keyword.control.at-rule",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "variable.css", "variable.other.css"],
      "settings": {
        "foreground": "#374151"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#362D59",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#4D0A55",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#362D59",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Invalid/Error",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF45A8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Declaration Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.module",
        "keyword.other.import",
        "keyword.other.package",
        "keyword.other.namespace",
        "storage.type.interface",
        "storage.type.type",
        "storage.type.class",
        "storage.type.function",
        "storage.type.const",
        "storage.type.let",
        "storage.type.var",
        "storage.type.enum",
        "storage.type.namespace",
        "storage.type.module",
        "storage.type.struct",
        "storage.type.trait",
        "storage.type.impl",
        "storage.type.def",
        "storage.modifier.import",
        "storage.modifier.package",
        "keyword.declaration",
        "keyword.function",
        "keyword.class",
        "keyword.interface",
        "keyword.struct",
        "keyword.control.def",
        "keyword.control.class",
        "keyword.other.fn",
        "keyword.other.def",
        "keyword.other.special-method",
        "meta.function.python keyword.python",
        "meta.class.python keyword.python",
        "source.go keyword.function",
        "source.go keyword.type",
        "source.go keyword.var",
        "source.go keyword.const",
        "source.rust storage.type",
        "keyword.other.important"
      ],
      "settings": {
        "foreground": "#b5006f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage fallback",
      "scope": ["storage"],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#4D0A55",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#EE8019",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#EE8019",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#4D0A55",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["markup.underline.link", "markup.underline.link.image"],
      "settings": {
        "foreground": "#362D59"
      }
    },
    {
      "name": "Markup - Raw/Code",
      "scope": ["markup.inline.raw", "markup.raw", "markup.raw.inline"],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Markup - Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF45A8"
      }
    },
    {
      "name": "Markup - Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#008900"
      }
    },
    {
      "name": "Markup - Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#bf8600"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#EE8019",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Meta Diff",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#6B7280"
      }
    },

    {
      "name": "Meta Embedded",
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#1F2937"
      }
    },
    {
      "name": "Punctuation Section",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.section.block",
        "punctuation.section.brackets",
        "punctuation.section.array",
        "punctuation.section.function"
      ],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "name": "Punctuation Terminator",
      "scope": ["punctuation.terminator", "punctuation.separator.statement"],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "name": "Entity Name Tag",
      "scope": ["entity.name.section", "entity.name.tag", "entity.name.namespace"],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "CSS Pseudo-classes and Pseudo-elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Library/Support Functions",
      "scope": ["support.function.builtin", "support.function.library"],
      "settings": {
        "foreground": "#6E47AE"
      }
    },
    {
      "name": "Invalid - Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#9CA3AF",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Regular Expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#226DFC"
      }
    },
    {
      "name": "Template Expression",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#4D0A55"
      }
    },
    {
      "name": "Language Variables",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.self",
        "variable.language.super",
        "keyword.other.this"
      ],
      "settings": {
        "foreground": "#7C3AED",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameter",
        "entity.name.variable.parameter"
      ],
      "settings": {
        "foreground": "#c97c00",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Diff Header",
      "scope": [
        "meta.diff.header",
        "meta.separator.diff",
        "meta.diff.index",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#226DFC"
      }
    }
  ]
}
