{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Sentry Sentinel Dark",
  "author": "Sentry",
  "themes": [
    {
      "name": "Sentinel Dark",
      "appearance": "dark",
      "style": {
        "background": "#181225",
        "foreground": "#F9F8F9",
